{
  "name": "sharable-kanban-boards",
  "displayName": "Shareable Kanban boards",
  "description": "Shareable Kanban boards for Visual Studio Code",
  "version": "1.1.2",
  "icon": "images/logo.png",
  "license": "GPL-3.0",
  "publisher": "iulian-radu-at",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=iulian-radu-at.sharable-kanban-boards",
  "repository": {
    "type": "git",
    "url": "https://github.com/iulian-radu-at/sharable-kanban-boards"
  },
  "bugs": {
    "url": "https://github.com/iulian-radu-at/sharable-kanban-boards/issues"
  },
  "engines": {
    "vscode": "1.59.0"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p tsconfig.ext.json",
    "build-webview": "react-app-rewired build && rimraf out/webview.js.LICENSE.txt && rimraf build",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "package": "vsce package",
    "install:local": "code --install-extension sharable-kanban-boards-1.1.1.vsix",
    "publish": "vsce publish"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Kanban",
    "Scram",
    "Project",
    "Management",
    "Todo",
    "Jira",
    "shareable"
  ],
  "activationEvents": [
    "onCommand:shareableKanbanBoards.create",
    "onCommand:shareableKanbanBoards.open",
    "onCommand:shareableKanbanBoards.delete"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "shareableKanbanBoards.create",
        "title": "Create a new board",
        "category": "Shareable Kanban boards"
      },
      {
        "command": "shareableKanbanBoards.open",
        "title": "Open a board",
        "category": "Shareable Kanban boards"
      },
      {
        "command": "shareableKanbanBoards.delete",
        "title": "Delete a board",
        "category": "Shareable Kanban boards"
      }
    ]
  },
  "devDependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/pickers": "3.3.10",
    "@types/mocha": "9.1.0",
    "@types/node": "14.17.12",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/uuid": "8.3.4",
    "@types/vscode": "1.59.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "cross-env": "7.0.3",
    "eslint": "8.8.0",
    "mocha": "9.2.0",
    "react": "17.0.2",
    "react-app-rewired": "2.1.11",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "4.5.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "vscode-test": "1.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
