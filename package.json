{
  "name": "sharable-kanban-boards",
  "displayName": "Shareable Kanban boards",
  "description": "Shareable Kanban boards for Visual Studio Code",
  "version": "1.1.3",
  "icon": "images/logo.png",
  "license": "GPL-3.0",
  "publisher": "iulian-radu-at",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=iulian-radu-at.sharable-kanban-boards",
  "repository": {
    "type": "git",
    "url": "https://github.com/iulian-radu-at/sharable-kanban-boards"
  },
  "bugs": {
    "url": "https://github.com/iulian-radu-at/sharable-kanban-boards/issues"
  },
  "sponsor": {
    "url": "https://www.patreon.com/iulian_radu_at"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Kanban",
    "Scram",
    "Project",
    "Management",
    "Todo",
    "Jira",
    "shareable"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p tsconfig.json",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src-ext --ext ts",
    "package": "vsce package",
    "install:local": "code --install-extension sharable-kanban-boards-1.1.3.vsix",
    "publish": "vsce publish"
  },
  "activationEvents": [
    "onCommand:shareableKanbanBoards.create",
    "onCommand:shareableKanbanBoards.open",
    "onCommand:shareableKanbanBoards.delete"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "shareableKanbanBoards.create",
        "title": "Create a new board",
        "category": "Shareable Kanban boards"
      },
      {
        "command": "shareableKanbanBoards.open",
        "title": "Open a board",
        "category": "Shareable Kanban boards"
      },
      {
        "command": "shareableKanbanBoards.delete",
        "title": "Delete a board",
        "category": "Shareable Kanban boards"
      }
    ]
  },
  "devDependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/pickers": "3.3.10",
    "@types/mocha": "9.1.1",
    "@types/node": "14.17.12",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/uuid": "8.3.4",
    "@types/vscode": "1.59.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "cross-env": "7.0.3",
    "eslint": "8.18.0",
    "mocha": "9.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4",
    "uglifyjs-webpack-plugin": "2.2.0",
    "vscode-test": "1.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "findUnusedExports": {
    "include": [
      "./src-ext/**"
    ]
  }
}
